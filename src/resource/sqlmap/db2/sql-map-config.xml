<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>

	<properties resource="sqlmap/service.properties" />

	<!--定义iBATIS自身应用信息-->
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="200" maxSessions="100"
		maxTransactions="20" useStatementNamespaces="true" />

	<!-- page 公共提取-->
	<sqlMap resource="sqlmap/db2/ibatis-page.xml" />
    <!-- 系统管理相关-->
	<sqlMap resource="sqlmap/db2/system/MenuInfo.xml" /> 
	<sqlMap resource="sqlmap/db2/system/UserInfo.xml" /> 
    <sqlMap resource="sqlmap/db2/system/OrganInfo.xml" />
    <sqlMap resource="sqlmap/db2/system/UserRole.xml" />
    <sqlMap resource="sqlmap/db2/system/SysvarInfo.xml" />
    <sqlMap resource="sqlmap/db2/system/RoleInfo.xml" />
    <sqlMap resource="sqlmap/db2/system/Parameter.xml" />
    <sqlMap resource="sqlmap/db2/system/ParamHis.xml" />

	
	<!-- 批量设置信息-->
	<sqlMap resource="sqlmap/db2/judicial/batchSet.xml" />
	<!-- 历史查询模板-->
	<sqlMap resource="sqlmap/db2/judicial/LscxMb.xml" /> 
	<!-- 历史条件设置-->
	<sqlMap resource="sqlmap/db2/judicial/lscxCon.xml" /> 
    <!-- 历史列设置-->
	<sqlMap resource="sqlmap/db2/judicial/lscxCol.xml" /> 
	 <!-- 历史列设置-->
	<sqlMap resource="sqlmap/db2/judicial/lscxDtzx.xml" /> 
	<!-- 历史查询申请-->
	<sqlMap resource="sqlmap/db2/judicial/lscxSq.xml" /> 
	<!-- 条件规则 -->
	<sqlMap resource="sqlmap/db2/judicial/lscxRul.xml" />
	<!-- 模板导出列明细 -->
	<sqlMap resource="sqlmap/db2/judicial/lscxExp.xml" />
	<!-- selcon -->
	<sqlMap resource="sqlmap/db2/judicial/selcon.xml" />
	<!-- 模板抬头明细 -->
	<sqlMap resource="sqlmap/db2/judicial/lscxHea.xml" />
	<!-- 日志 -->
	<sqlMap resource="sqlmap/db2/judicial/lscxLog.xml" />
	<!-- 申请的审核日志 -->
	<sqlMap resource="sqlmap/db2/judicial/lscxSqhis.xml" />
	<!-- 通用选取配置-->
	<sqlMap resource="sqlmap/db2/judicial/selSet.xml" />
	<!-- 批量查询相关-->
	<sqlMap resource="sqlmap/db2/judicial/plcx.xml" />
	<!-- 司法查控相关-->
	<sqlMap resource="sqlmap/db2/judicial/sfck.xml" />
	<!-- 查询登记相关-->
	<sqlMap resource="sqlmap/db2/judicial/djcx.xml" />
	<!-- 法院司法查控相关-->
	<sqlMap resource="sqlmap/db2/judicial/fyck.xml" />
	<!-- 人民银行电信诈骗相关-->
	<sqlMap resource="sqlmap/db2/judicial/jymx.xml" />
	<!-- 人民银行电信诈骗止付结冻相关-->
	<sqlMap resource="sqlmap/db2/judicial/zfdj.xml" />
	<!-- 公安厅司法查控相关-->
	<sqlMap resource="sqlmap/db2/judicial/gatck.xml" />
	
</sqlMapConfig>