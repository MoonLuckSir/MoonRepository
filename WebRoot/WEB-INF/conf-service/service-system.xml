<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd"
	default-autowire="byName">
	
	<!-- 批量设置 -->
	<bean id="batchSetService"
		class="com.yitong.app.service.judicial.batchset.BatchSetService">
	</bean>
	
	<bean id="loginService"
		class="com.yitong.app.service.system.LoginService">
	</bean>

	<bean id="menuService"
		class="com.yitong.app.service.system.MenuService">
	</bean>

	<bean id="organService"
		class="com.yitong.app.service.system.OrganService">
	</bean>

	<bean id="roleService"
		class="com.yitong.app.service.system.RoleService">
	</bean>
	
	<bean id="userService"
		class="com.yitong.app.service.system.UserService">
	</bean>
	
	<bean id="sysvarService"
		class="com.yitong.app.service.system.SysvarService">
	</bean> 
		
	<bean id="paramHisService"
		class="com.yitong.app.service.system.ParamHisService">
	</bean> 
	
</beans>